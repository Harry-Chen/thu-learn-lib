{
  "name": "thu-learn-lib",
  "version": "4.2.2",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "license": "MIT",
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "jsdelivr": "lib/index.iife.js",
  "unpkg": "lib/index.iife.js",
  "module": "./lib/index.js",
  "browser": "lib/index.iife.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "vitest run"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "entities": "^7.0.0",
    "js-base64": "^3.7.8",
    "node-fetch-native": "^1.6.7",
    "sm-crypto": "^0.3.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "*",
    "@types/sm-crypto": "^0.3.4",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.16.0",
  "publishConfig": {
    "exports": {
      ".": "./lib/index.js",
      "./package.json": "./package.json"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "entities": "^7.0.0",
      "parse5": "npm:fake-parse5",
      "parse5-htmlparser2-tree-adapter": "npm:fake-parse5"
    }
  }
}
