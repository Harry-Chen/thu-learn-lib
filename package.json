{
  "name": "thu-learn-lib",
  "version": "4.2.6",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "license": "MIT",
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "jsdelivr": "lib/index.iife.js",
  "unpkg": "lib/index.iife.js",
  "browser": "lib/index.iife.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "vitest run"
  },
  "dependencies": {
    "cheerio": "^1.2.0",
    "entities": "^7.0.1",
    "js-base64": "^3.7.8",
    "node-fetch-native": "^1.6.7",
    "sm-crypto": "^0.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.1",
    "@types/sm-crypto": "^0.3.4",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.2",
  "publishConfig": {
    "exports": {
      ".": "./lib/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
