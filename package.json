{
  "name": "thu-learn-lib",
  "version": "4.1.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "jsdelivr": "lib/index.global.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "22.13.10",
    "@types/sm-crypto": "^0.3.4",
    "domhandler": "^5.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "fake-parse5": "^0.0.1",
    "prettier": "^3.5.3",
    "terser": "^5.39.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "entities": "4.5.0",
    "fetch-cookie": "^3.1.0",
    "js-base64": "^3.7.7",
    "node-fetch-cookie-native": "^0.0.2",
    "node-fetch-native": "^1.6.6",
    "sm-crypto": "^0.3.13",
    "tough-cookie": "^5.1.2"
  },
  "packageManager": "pnpm@10.6.4+sha512.da3d715bfd22a9a105e6e8088cfc7826699332ded60c423b14ec613a185f1602206702ff0fe4c438cb15c979081ce4cb02568e364b15174503a63c7a8e2a5f6c",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
