{
  "name": "thu-learn-lib",
  "version": "1.1.4",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build-lib": "tsc",
    "build-dist": "webpack --config webpack.config.js",
    "watch-dist": "webpack -w --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/chrome": "0.0.86",
    "@types/entities": "^1.1.0",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^24.9.0",
    "@types/js-base64": "^2.3.1",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.16.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cross-fetch": "^3.0.1",
    "entities": "^2.0.0",
    "es6-denodeify": "^0.1.5",
    "form-data": "^3.0.0",
    "js-base64": "^2.5.1",
    "real-isomorphic-fetch": "^3.0.0"
  }
}
